#include <stdio.h>

int main() {
    int n, i;
    int first = 0, second = 1, next;

    printf("Enter the number of terms: ");
    scanf("%d", &n);

    printf("Fibonacci Series: ");

    for(i = 1; i <= n; i++) {
        printf("%d ", first);

        // Calculate next term
        next = first + second;

        // Update values
        first = second;
        second = next;
    }

    return 0;
}
